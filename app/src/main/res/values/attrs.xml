<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CountDownView">
        <attr name="cdv_textsize" format="dimension"/>
        <attr name="cdv_textcolor" format="color"/>
        <attr name="cdv_rectbgcolor" format="color"/>
        <attr name="cdv_countdowncolor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ArrowView">
        <attr name="arrow_direction" format="integer"/>
        <attr name="arrow_style">
            <enum name="up" value="0"/>
            <enum name="down" value="1"/>
            <enum name="left" value="2"/>
            <enum name="right" value="3"/>

        </attr>
    </declare-styleable>
</resources>